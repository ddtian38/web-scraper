  {{#each articles}}
        <div class="row">
        <div class="col s12 m6">
        <div class="card medium" data-id="{{this._id}}">
            <div class="card-image">
            <img src="{{this.img}}">
            <span class="card-title">{{this.title}}</span>
            </div>
            <div class="card-content">
            <p>Summary: {{this.summary}}</p>
            </div>
        </div>
        </div>
    </div>
  {{/each}}